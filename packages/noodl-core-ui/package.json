{
  "name": "@noodl/noodl-core-ui",
  "version": "2.7.0",
  "scripts": {
    "start": "start-storybook -p 6006 -s public",
    "build": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "classnames": "^2.5.1"
  },
  "peerDependencies": {
    "@noodl/platform": "file:../noodl-platform",
    "react": "19.0.0",
    "react-dom": "19.0.0"
  },
  "devDependencies": {
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.42",
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0",
    "babel-plugin-named-exports-order": "^0.0.2",
    "prop-types": "^15.8.1",
    "sass": "^1.90.0",
    "storybook": "^9.1.3",
    "ts-loader": "^9.5.4",
    "typescript": "^4.9.5",
    "web-vitals": "^3.5.2",
    "webpack": "^5.101.3"
  }
}
